datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Playlist {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  genre       String
  mood        String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}